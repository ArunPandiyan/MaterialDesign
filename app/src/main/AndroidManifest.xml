<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="technibits.com.pme">

    <application
        android:name=".alarmactivity.RemindMe"
        android:allowBackup="true"

        android:icon="@mipmap/app_logo"
        android:label="@string/app_name"
        android:theme="@style/MyMaterialTheme.Base">
        <activity
            android:name="technibits.com.pme.activity.LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="technibits.com.pme.activity.MainActivity"

            android:label="@string/app_name"></activity>
        <activity
            android:name="technibits.com.pme.activity.QuizActivity"
            android:label="@string/app_name"></activity>

        <activity
            android:name="technibits.com.pme.activity.ModeActivity"
            android:label="@string/app_name"
            android:parentActivityName="technibits.com.pme.activity.MainActivity"></activity>

        <activity
            android:name="technibits.com.pme.activity.MainActivity_as"
            android:label="@string/app_name"
            android:parentActivityName="technibits.com.pme.activity.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="technibits.com.pme.activity.MainActivity" />


        </activity>
        <activity
            android:name="technibits.com.pme.activity.CreateAccountActivity"
            android:label="@string/app_name"></activity>
        <activity
            android:name="technibits.com.pme.activity.DashboardActivity"
            android:label="@string/app_name"></activity>
        <activity
            android:name="technibits.com.pme.activity.PerformanceActivity"
            android:label="@string/app_name"></activity>
        <activity
            android:name="technibits.com.pme.activity.ReviewHisActivity"
            android:label="@string/app_name"></activity>
        <activity
            android:name="technibits.com.pme.activity.PreviewActivity"
            android:label="@string/app_name"
            />

        <activity
            android:name="technibits.com.pme.activity.SettingActivity"
            android:label="@string/app_name"></activity>

        <activity
            android:name="technibits.com.pme.activity.ToolBarAnim"
            android:label="@string/app_name"></activity>
        <activity
            android:name="technibits.com.pme.alarmactivity.MainActivity"
            android:label="@string/app_name"></activity>
        <activity
            android:name="technibits.com.pme.alarmactivity.AddAlarmActivity"
            android:windowSoftInputMode="stateHidden"></activity>
        <activity android:name="technibits.com.pme.alarmactivity.SettingsActivity"></activity>
        <activity android:name="technibits.com.pme.activity.VideoListDemoActivity"></activity>

        <receiver android:name="technibits.com.pme.alarmactivity.AlarmSetter">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>
        <receiver android:name="technibits.com.pme.alarmactivity.AlarmReceiver"></receiver>

        <service android:name="technibits.com.pme.alarmactivity.AlarmService"></service>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.USE_CREDENTIALS" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.VIBRATE" />

</manifest>
